{
  "version": 2,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" },
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "*.png", "use": "@vercel/static" },
    { "src": "site.webmanifest", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "login.html" },
    { "src": "/(.*)", "dest": "server.js" }
  ]
}

